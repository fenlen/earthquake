<p id="notice"><%= notice %></p>

<h1>Tsunamis</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Samedayas</th>
      <th>Focal depth</th>
      <th>Primary magnitude</th>
      <th>Country</th>
      <th>Location name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Maximun water height</th>
      <th>Total deaths</th>
      <th>Total injuries</th>
      <th>Total damage million dollars</th>
      <th>Total houses destroyed</th>
      <th>Total houses damaged</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tsunamis.each do |tsunami| %>
      <tr>
        <td><%= tsunami.Date %></td>
        <td><%= tsunami.Time %></td>
        <td><%= tsunami.SameDayAs %></td>
        <td><%= tsunami.Focal_Depth %></td>
        <td><%= tsunami.Primary_Magnitude %></td>
        <td><%= tsunami.Country %></td>
        <td><%= tsunami.Location_Name %></td>
        <td><%= tsunami.Latitude %></td>
        <td><%= tsunami.Longitude %></td>
        <td><%= tsunami.Maximun_Water_Height %></td>
        <td><%= tsunami.Total_Deaths %></td>
        <td><%= tsunami.Total_Injuries %></td>
        <td><%= tsunami.Total_Damage_Million_Dollars %></td>
        <td><%= tsunami.Total_Houses_Destroyed %></td>
        <td><%= tsunami.Total_Houses_Damaged %></td>
        <td><%= link_to 'Show', tsunami %></td>
        <td><%= link_to 'Edit', edit_tsunami_path(tsunami) %></td>
        <td><%= link_to 'Destroy', tsunami, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tsunami', new_tsunami_path %>
